<form action="/landmarks/<%=@landmark.id%>" method="POST">
  <input type="hidden" name="_method" value="patch">
  <h1>Change Info Below to Update <%=@landmark.name%></h1>
    <label>Name:</label> <input type="text" name="name" id="landmark[name]" value="<%= @landmark.name%>">
    <label>Year Completed:</label> <input type="text" name="year_completed" id="landmark[year_completed]" value="<%= @landmark.year_completed%>">

    <input type="submit" value="Edit Landmark">
</form>
